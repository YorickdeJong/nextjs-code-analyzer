{
  "exclude": [
    "node_modules",
    "implementations/cpp",
    ".vscode-test",
    "nextjs-code-analyzer-extension"
  ],
  "compilerOptions": {
    "types": ["node", "jest"],
    "module": "commonjs",
    "target": "es6",
    "outDir": "out",
    "lib": ["es6"],
    "esModuleInterop": true,
    "sourceMap": true,
    "rootDir": "./",
    "strict": true
  },
  "include": ["./**/*.ts"],
}
